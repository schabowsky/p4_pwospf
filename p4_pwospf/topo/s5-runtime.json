{
  "target": "bmv2",
  "p4info": "build/program.p4.p4info.txt",
  "bmv2_json": "build/program.json",
  "table_entries": [
    {
      "table": "MyIngress.inport_table",
      "match": {
        "standard_metadata.ingress_port": 1
      },
      "action_name": "MyIngress.set_cpu_meta",
      "action_params": {
        "inPort": "00:00:00:00:05:01"
      }
    },
    {
      "table": "MyIngress.inport_table",
      "match": {
        "standard_metadata.ingress_port": 2
      },
      "action_name": "MyIngress.set_cpu_meta",
      "action_params": {
        "inPort": "00:00:00:00:05:02"
      }
    },
    {
      "table": "MyIngress.inport_table",
      "match": {
        "standard_metadata.ingress_port": 3
      },
      "action_name": "MyIngress.set_cpu_meta",
      "action_params": {
        "inPort": "00:00:00:00:05:03"
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.2.2", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "08:00:00:00:02:22",
        "port": 1
      }
    },
    {
      "table": "MyEgress.egress_table",
      "match": {
        "standard_metadata.egress_port": 1
      },
      "action_name": "MyEgress.set_ospf_src",
      "action_params": {
        "srcAddr": "10.0.2.15"
      }
    },
    {
      "table": "MyEgress.egress_table",
      "match": {
        "standard_metadata.egress_port": 2
      },
      "action_name": "MyEgress.set_ospf_src",
      "action_params": {
        "srcAddr": "10.0.8.15"
      }
    },
    {
      "table": "MyEgress.egress_table",
      "match": {
        "standard_metadata.egress_port": 3
      },
      "action_name": "MyEgress.set_ospf_src",
      "action_params": {
        "srcAddr": "10.0.7.15"
      }
    },
    {
      "table": "MyIngress.mcg_table",
      "match": {
        "hdr.ipv4.dstAddr": "224.0.0.5"
      },
      "action_name": "MyIngress.set_mcg",
      "action_params": {
        "mcast_group": 1
      }
    }
  ],
  "multicast_group_entries" : [
    {
      "multicast_group_id" : 1,
      "replicas" : [
        {
          "egress_port" : 1,
          "instance" : 1
        },
        {
          "egress_port" : 2,
          "instance" : 1
        },
        {
          "egress_port" : 3,
          "instance" : 1
        }
      ]
    }
  ]
}
